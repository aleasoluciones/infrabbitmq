#!/bin/bash

virtualenv --system-site-packages ic_env
source ./ic_env/bin/activate

export BROKER_URI=amqp://guest:guest@192.168.10.21/

python setup.py develop
pip install -r requirements.txt --index-url=http://pypi.alea-soluciones.com/distutils
pip install -r requirements-dev.txt --index-url=http://pypi.alea-soluciones.com/distutils

nosetests tests integration_tests
